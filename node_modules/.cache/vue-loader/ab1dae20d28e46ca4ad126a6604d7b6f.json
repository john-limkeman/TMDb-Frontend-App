      chosenItem: [],\n        query: {\n          text: \"\",\n          page: 1,\n          type: \"movie\",\n        },\n        api: {\n          base: \"https://api.themoviedb.org/3\",\n          key: process.env.VUE_APP_API_KEY,\n          params: \"&language=en-US&include_adult=false&query=\",\n          page: \"&page=\"\n        }\n    }\n  },\n  methods: {\n    conductSearch(){\n      console.log(this.query)\n      if(this.query.type == \"movie\"){\n        this.searchMovies();\n      } else if (this.query.type == \"tv\"){\n        this.searchTV();\n      } else{\n        this.searchPeople();\n      }\n    },\n    searchMovies(){\n      console.log(\"movie search...\")\n      try{\n        fetch(`${this.api.base}${this.api.key}${this.api.param}${this.query.text}${this.api.page}${this.query.page}`)\n        .then(res => res.json).then(result =>{\n          this.results = result.results;\n        })\n      } catch(error){\n        console.log(\"error\")\n      }\n    },\n    searchTV(){\n      console.log(\"tv search...\")\n    },\n    searchPeople(){\n      console.log(\"people search...\")\n    }\n\n  },\n  components: {\n\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n\n}\n  #logo{\nheight: 200px;\nwidth: auto;\n  }\n</style>\n"]}]}